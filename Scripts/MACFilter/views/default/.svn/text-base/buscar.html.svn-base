{{extend 'layout.html'}}

<h1> Resultado </h1>

<hr />

{{if len(devices)>1:}}
<table>
  <tr>
   <td><b>MAC</td>
   <td><b>Nombre</td>
   <td><b>Acciones</td>
  </tr>


  {{for row in devices:}}
    <tr>
     <td>{{=row.mac}}</td>
     <td>{{=row.name}}</td>
     <td>
       {{if row.vlan.name != "700" and row.vlan.name != "702":}}
         <a href="{{=URL('data',args=['delete','device',row.device.id])}}">BORRAR</a> | <a href="{{=URL('data',args=['update','device',row.device.id])}}">MODIFICAR</a>
       {{else:}}
         NO MODIFICABLE
       {{pass}}
     </td>
    </tr>

  {{pass}}
  </table>
{{else:}}
 <b>No hay resultados</b>
{{pass}}
